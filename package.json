{
  "name": "nw-london-local-ledger",
  "version": "0.1.0",
  "description": "A comprehensive, data-driven community hub for North West London property and planning information",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:isr": "node scripts/build-isr.js",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "node scripts/database/migrate.js",
    "db:seed": "node scripts/database/seed.js",
    "db:reset": "node scripts/database/reset.js",
    "scraper:run": "node scripts/scrapers/run-all.js",
    "scraper:council": "node scripts/scrapers/run-council.js",
    "scraper:parallel": "tsx src/scrapers/run-parallel-scrapers.ts",
    "scraper:parallel:build": "tsc src/scrapers/run-parallel-scrapers.ts --outDir dist/scrapers",
    "scraper:comprehensive": "tsx src/scrapers/run-comprehensive-scraping.ts",
    "scraper:transport": "tsx src/scrapers/run-comprehensive-scraping.ts --skip-councils --skip-energy --skip-crime --skip-amenities",
    "scraper:energy": "tsx src/scrapers/run-comprehensive-scraping.ts --skip-councils --skip-transport --skip-crime --skip-amenities",
    "scraper:crime": "tsx src/scrapers/run-comprehensive-scraping.ts --skip-councils --skip-transport --skip-energy --skip-amenities",
    "scraper:amenities": "tsx src/scrapers/run-comprehensive-scraping.ts --skip-councils --skip-transport --skip-energy --skip-crime",
    "generate:sitemap": "node scripts/seo/generate-sitemap.js",
    "generate:pages": "node scripts/seo/generate-pages.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "worker:start": "tsx src/workers/queue-worker.ts",
    "worker:dev": "tsx watch src/workers/queue-worker.ts",
    "queue:stats": "curl http://localhost:3000/api/admin/queues/stats",
    "queue:health": "curl http://localhost:3000/api/admin/queues/health"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "postgres": "^3.4.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "@elastic/elasticsearch": "^8.10.0",
    "openai": "^4.20.0",
    "zod": "^3.22.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.5.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "winston": "^3.11.0",
    "bullmq": "^4.14.0",
    "sharp": "^0.32.0",
    "clsx": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "@apollo/server": "^4.9.0",
    "graphql": "^16.8.0",
    "graphql-tag": "^2.12.6",
    "dataloader": "^2.2.2",
    "@as-integrations/next": "^3.0.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-query-complexity": "^0.12.0",
    "graphql-rate-limit": "^3.3.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@playwright/test": "^1.40.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.0",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "tsx": "^4.6.0",
    "dotenv": "^16.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nw-london-local-ledger.git"
  },
  "keywords": [
    "property",
    "planning",
    "london",
    "real-estate",
    "local-news",
    "nw-london",
    "seo",
    "programmatic-seo"
  ],
  "author": "NW London Local Ledger Team",
  "license": "PROPRIETARY"
}
