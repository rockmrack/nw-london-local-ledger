{
  "name": "nw-london-local-ledger",
  "version": "0.1.0",
  "description": "Hampstead Renovations - Your Trusted Partner for North West London Property Information. Comprehensive property data, planning applications, and local area insights.",
  "private": true,
  "author": "NW London Local Ledger Team",
  "homepage": "https://www.hampsteadrenovations.co.uk",
  "bugs": {
    "email": "contact@hampsteadrenovations.co.uk"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:isr": "node scripts/build-isr.js",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "node scripts/database/migrate.js",
    "db:seed": "node scripts/database/seed.js",
    "db:reset": "node scripts/database/reset.js",
    "scraper:run": "node scripts/scrapers/run-all.js",
    "scraper:council": "node scripts/scrapers/run-council.js",
    "scraper:parallel": "tsx src/scrapers/run-parallel-scrapers.ts",
    "scraper:parallel:build": "tsc src/scrapers/run-parallel-scrapers.ts --outDir dist/scrapers",
    "scraper:comprehensive": "tsx src/scrapers/run-comprehensive-scraping.ts",
    "scraper:transport": "tsx src/scrapers/run-comprehensive-scraping.ts --skip-councils --skip-energy --skip-crime --skip-amenities",
    "scraper:energy": "tsx src/scrapers/run-comprehensive-scraping.ts --skip-councils --skip-transport --skip-crime --skip-amenities",
    "scraper:crime": "tsx src/scrapers/run-comprehensive-scraping.ts --skip-councils --skip-transport --skip-energy --skip-amenities",
    "scraper:amenities": "tsx src/scrapers/run-comprehensive-scraping.ts --skip-councils --skip-transport --skip-energy --skip-crime",
    "generate:sitemap": "node scripts/seo/generate-sitemap.js",
    "generate:pages": "node scripts/seo/generate-pages.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "worker:start": "tsx src/workers/queue-worker.ts",
    "worker:dev": "tsx watch src/workers/queue-worker.ts",
    "queue:stats": "curl http://localhost:3000/api/admin/queues/stats",
    "queue:health": "curl http://localhost:3000/api/admin/queues/health",
    "wasm:build": "cd rust-wasm && ./build.sh",
    "wasm:build:property": "cd rust-wasm/property-processor && wasm-pack build --target web --out-dir ../../src/lib/wasm/pkg/property-processor --release",
    "wasm:build:geo": "cd rust-wasm/geo-calculator && wasm-pack build --target web --out-dir ../../src/lib/wasm/pkg/geo-calculator --release",
    "wasm:build:stats": "cd rust-wasm/stats-engine && wasm-pack build --target web --out-dir ../../src/lib/wasm/pkg/stats-engine --release",
    "wasm:build:search": "cd rust-wasm/search-optimizer && wasm-pack build --target web --out-dir ../../src/lib/wasm/pkg/search-optimizer --release",
    "wasm:build:transform": "cd rust-wasm/data-transformer && wasm-pack build --target web --out-dir ../../src/lib/wasm/pkg/data-transformer --release",
    "wasm:test": "cd rust-wasm && cargo test",
    "wasm:benchmark": "curl http://localhost:3000/api/wasm-benchmark?size=10000&iterations=100",
    "wasm:clean": "rm -rf src/lib/wasm/pkg && cd rust-wasm && cargo clean",
    "analyze": "ANALYZE=true npm run build",
    "lighthouse": "lhci autorun",
    "perf:check": "node scripts/check-performance-budgets.js",
    "workers:deploy": "wrangler publish workers/multi-region.ts"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/next": "^3.0.0",
    "@elastic/elasticsearch": "^8.10.0",
    "@neshca/cache-handler": "^1.0.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-backend-wasm": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.3",
    "bullmq": "^4.14.0",
    "cheerio": "^1.0.0-rc.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dataloader": "^2.2.2",
    "date-fns": "^2.30.0",
    "graphql": "^16.8.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-query-complexity": "^0.12.0",
    "graphql-rate-limit": "^3.3.0",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "lru-cache": "^10.1.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.13",
    "openai": "^4.20.0",
    "pg": "^8.11.0",
    "postcss": "^8.4.0",
    "postgres": "^3.4.0",
    "puppeteer": "^21.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.6.0",
    "sharp": "^0.32.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.2.0",
    "winston": "^3.11.0",
    "workbox-webpack-plugin": "^7.0.0",
    "workbox-window": "^7.0.0",
    "worker-loader": "^3.0.8",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@next/bundle-analyzer": "^14.0.0",
    "@playwright/test": "^1.40.0",
    "@size-limit/preset-app": "^11.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "dotenv": "^16.3.0",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "size-limit": "^11.0.0",
    "tsx": "^4.6.0",
    "webpagetest": "^0.5.0",
    "wrangler": "^3.22.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nw-london-local-ledger.git"
  },
  "keywords": [
    "property",
    "planning",
    "london",
    "real-estate",
